[
  {
    "id": "01",
    "name": "GetFullname",
    "description": "Concatinates user.firstName and user.lastName",
    "conditions": [
     {
      "type": "all",
      "comparators": [
          {
              "field": "user.age",
              "comparatorType": ">",
              "value": 18
          }
      ],
      "subconditions": []
      }
    ],
    "actions": [
      {
          "field": "user.CanDrink",
          "actionType": "create",
          "expression": "YES"
      },
      {
          "field": "user.CanDrive",
          "actionType": "create",
          "expression": "YES"
      },
      {
          "field": "user.CanDrinkAndDrive",
          "actionType": "create",
          "expression": "NOO"
      },
      {
          "field": "user.fullName",
          "actionType": "create",
          "expression": "${user.firstName} ${user.lastName}"
      }
    ]
  }
]